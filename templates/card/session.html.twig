{% extends "base.html.twig" %}

{% block title %}Card game{% endblock %}

{% block main %}
<main class="main">
<h1>Session keys:</h1>
{{ include('flash.html.twig') }}
{% for key, value in app.session %}
<p>{{ key }}</p>
{% endfor %}
<button class="button"><a href="{{ path('session_delete') }}">Delete session</a></button>
<button class="button"><a href="{{ path('card_start') }}">Back</a></button>
</main>
{% endblock %}
